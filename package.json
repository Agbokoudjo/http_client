{
  "name": "@wlindabla/http_client",
  "author": "AGBOKOUJO Franck <internationaleswebservices@gmail.com>",
  "version": "1.1.0",
  "description": "Enterprise-grade HTTP client for Node.js and browsers. Event-driven architecture inspired by Symfony HttpKernel. Features: interceptors, retry logic, circuit breaker, caching, TypeScript support. Zero dependencies. Works everywhere.Universal HTTP client with superpowers! Event-driven lifecycle • Smart retry & timeout • Request/Response interceptors • Full TypeScript • Circuit breaker • Works in Node.js & browsers • Zero dependencies • Production-ready .Stop fighting with HTTP requests. @wlindabla/http_client gives you full control with events, interceptors, caching, and retry logic. Inspired by Symfony. TypeScript-first. Works everywhere. Built for production. Symfony HttpKernel architecture meets modern HTTP client. Event-driven lifecycle (REQUEST→BEFORE_SEND→RESPONSE→ERROR→TERMINATE). Delegates, interceptors, circuit breaker. Universal (Node.js + browsers). TypeScript. Production-grade.",
  "type": "module",
  "keywords": [
    "http",
    "client",
    "fetch",
    "universal",
    "isomorphic",
    "http-client",
    "typescript",
    "NodeJs",
    "NodeJs-fetch",
    "http-client",
    "axios-alternative",
    "typescript",
    "nodejs",
    "browser",
    "event-driven",
    "interceptors",
    "circuit-breaker",
    "symfony",
    "microservices",
    "rest-api",
    "universal",
    "isomorphic",
    "enterprise",
    "rest",
    "client",
    "axios",
    "request",
    "httpclient",
    "restful",
    "got",
    "ky",
    "superagent",
    "api"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Agbokoudjo/http_client.git"
  },
  "bugs": {
    "url": "https://github.com/Agbokoudjo/http_client/issues"
  },
  "homepage": "https://github.com/Agbokoudjo/http_client#readme",
  "packageManager": "yarn@4.12.0",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:node": "vitest run --environment node",
    "test:browser": "vitest run --environment jsdom",
    "test:integration": "vitest run tests/integration.test.ts",
    "test:unit": "vitest run tests/unit",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=json --outputFile=test-results.json",
    "build": "yarn cache clean && tsup",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint src --ext .ts --fix",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --outDir dist/types",
    "clean": "rm -rf dist coverage test-results.json",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn typecheck && yarn build && yarn test && yarn lint",
    "prepare": "yarn build",
    "release": "yarn build && changeset publish"
  },
  "dependencies": {
    "@wlindabla/event_dispatcher": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
